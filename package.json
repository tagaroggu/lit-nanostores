{
  "name": "lit-nanostores",
  "version": "0.1.0",
  "description": "A Lit controller for listening to nanostores",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./nanostores": {
      "import": "./dist/nanostores.js",
      "types": "./dist/nanostores.d.ts"
    }
  },
  "scripts": {
    "build": "tsc"
  },
  "keywords": [
    "lit",
    "nanostores"
  ],
  "author": "tagaroggu",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "lit": "^2.5",
    "nanostores": "^0.7"
  },
  "peerDependenciesMeta": {
    "lit": {
      "optional": false
    },
    "nanostores": {
      "optional": true
    }
  }
}
