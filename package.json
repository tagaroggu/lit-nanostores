{
  "name": "lit-stores",
  "version": "0.1.0",
  "description": "A Lit controller for listening to nanostores",
  "type": "module",
  "main": "./dist/lit-stores.js",
  "types": "./dist/lit-stores.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lit-stores.js",
      "require": "./dist/lit-stores.cjs",
      "types": "./dist/lit-stores.d.ts"
    },
    "./nanostores": {
      "import": "./dist/nanostores.js",
      "require": "./dist/nanostores.cjs",
      "types": "./dist/nanostores.d.ts"
    },
    "./vue-reactivity": {
      "import": "./dist/vue-reactivity.js",
      "require": "./dist/vue-reactivity.cjs",
      "types": "./dist/vue-reactivity.d.ts"
    },
    "./preact-signals": {
      "import": "./dist/preact-signals.js",
      "require": "./dist/preact-signals.cjs",
      "types": "./dist/preact-signals.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite dev"
  },
  "keywords": [
    "lit",
    "nanostores"
  ],
  "author": "tagaroggu",
  "license": "MIT",
  "devDependencies": {
    "@preact/signals-core": "^1.2.3",
    "@vue/reactivity": "^3.2.47",
    "lit": "^2.6.1",
    "nanostores": "^0.7.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.2"
  },
  "peerDependencies": {
    "@preact/signals-core": "^1.2.3",
    "@vue/reactivity": "^3.2",
    "lit": "^2.5",
    "nanostores": "^0.7"
  },
  "peerDependenciesMeta": {
    "lit": {
      "optional": false
    },
    "nanostores": {
      "optional": true
    },
    "@vue/reactivity": {
      "optional": true
    },
    "@preact/signals-core": {
      "optional": true
    }
  }
}
